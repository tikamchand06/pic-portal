{
  "name": "TCM - Unsplash",
  "short_name": "TCM - Unsplash",
  "version": "1.0.2",
  "manifest_version": 2,
  "description": "Download HD images freely from your browser.",
  "author": "TIKAM CHAND",
  "homepage_url": "http://tcmhack.in",
  "icons": {
    "128": "icon.png",
    "16": "icon.png",
    "22": "icon.png",
    "32": "icon.png",
    "48": "icon.png"
  },
  "browser_action": {
    "default_title": "TCM - Unsplash",
    "default_icon": "icon.png",
    "default_popup": "index.html"
  },
  "permissions": ["https://unsplash.com/", "http://unsplash.com/"]
}
